---
import Layout from "../../layouts/MainLayout.astro";
import { JSee as JSeeSolid } from "../../components/jsee";
import PageHeader from "../../components/pageHeader.astro";

const meta = {
  title: "JSee",
  description: "The Json Seer with magic powers",
}
---

<Layout title={meta.title} meta={meta}>
  <PageHeader {...meta} />
  <main>
    <JSeeSolid client:load />
  </main>
</Layout>

<style>
  main {
    display: flex;
    flex-direction: column;
    min-height: 100vh;
    gap: 2rem;
    width: 100%;
  }
</style>
